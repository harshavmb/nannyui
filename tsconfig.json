{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "allowJs": true,
    "noUnusedLocals": false,
    "strictNullChecks": false,
    "esModuleInterop": true, //https://stackoverflow.com/questions/55227775/react-undefined-for-react-router-with-typescript
    "allowSyntheticDefaultImports": true //https://stackoverflow.com/questions/55227775/react-undefined-for-react-router-with-typescript
  }
}
